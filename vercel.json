{
  "version": 2,
  "version": 3,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",

  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/vercel.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
      "dest": "server/vercel.ts"
    }
  ]
}
